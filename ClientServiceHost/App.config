<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <services>
      <service name="ClientService.ClientService" behaviorConfiguration="mexBehavior">
        <endpoint address="ClientService" binding="basicHttpBinding" contract="ClientService.IClientService"></endpoint>
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:9998" />
          </baseAddresses>
        </host>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="mexBehavior">
          <serviceMetadata httpGetEnabled="true" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
</configuration>
